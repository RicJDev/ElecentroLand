proceso validacion (e:id_cliente_reservacion:entero,s:existe_usuario:entero,cantidad_titulares[i_cliente]:entero)
Inicio
Var existe_usuario=entero

Para (i_cliente = 1 Hasta i_cliente = 500) Hacer;
    Si (id_cliente_reservacion	= cantidad_titulares[i_cliente]) Entonces;
      existe_usuario = 1;
	 sino 
	  existe_usuario=0;
    Fin_Si
  Fin_Para
  devolver(existe_usuario);
fin funcion
